<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="pop_anim_bottom_appear">
        <item name="android:windowEnterAnimation">@anim/anim_statebar_enter</item>
        <item name="android:windowExitAnimation">@anim/anim_statebar_exit</item>
    </style>

    <style name="check_box_style" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/xml_collect_sector</item>
    </style>

    <style name="progress_horizonal" parent="@android:style/Widget.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/xml_progress_horizontal</item>
        <item name="android:minHeight">2dip</item>
        <item name="android:maxHeight">2dip</item>
    </style>
    
    <style name="nav_button" parent="@android:style/Widget.Button">
        <item name="android:minHeight">48dp</item>
        <item name="android:textColor">#333</item>
        <item name="android:textSize">18sp</item>
        <item name="android:background">@drawable/xml_ib_bk</item>
    </style>
    
    

    <style name="theme_fund.Widget.Sherlock.Light.ActionBar.Solid.Inverse" parent="@style/Widget.Sherlock.Light.ActionBar">
        <item name="homeAsUpIndicator">@drawable/abs__ic_ab_back_holo_dark</item>
        <item name="titleTextStyle">@style/TextAppearance.Sherlock.Widget.ActionBar.Title.Inverse</item>
        <item name="subtitleTextStyle">@style/TextAppearance.Sherlock.Widget.ActionBar.Subtitle.Inverse</item>
    </style>

    <style name="theme_fund.spinnerItemStyle">
        <item name="android:textAppearance">@style/theme_fund.SpinnerItem</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
    </style>

    <style name="theme_fund.SpinnerItem">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="theme_fund.SearchAutoCompleteTextView" parent="@style/Widget.Sherlock.Light.SearchAutoCompleteTextView">
        <item name="android:focusable">true</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textColorHint">#cccccc</item>
    </style>

    <style name="theme_fund.ActionMode" parent="@style/Widget.Sherlock.Light.ActionMode.Inverse"></style>

    <style name="RatingBarStyle" parent="@android:style/Widget.RatingBar">
         <item name="android:minHeight">21dip</item>
        <item name="android:maxHeight">21dip</item>
        <item name="android:progressDrawable">@drawable/xml_rating_bar</item>
        <item name="android:indeterminateDrawable">@drawable/xml_rating_bar</item>
    </style>

    <style name="EditTextrenzh" parent="android:Widget.EditText">
        <item name="android:background">@drawable/renzh_edit_text_holo_light</item>
        <item name="android:textColor">#000000</item>
    </style>

</resources>